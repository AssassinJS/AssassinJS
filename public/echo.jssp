<html>
<head>
</head>
<body>
<$
var qs = require('querystring');
req = __request;
res = __response;

var formbody = qs.parse(req.body);
$>
<h2>Request Headers:</h2>
<$=JSON.stringify(req.headers)$><br /><br />
<h2>Request Body:</h2>
<$=JSON.stringify(req.body)$><br /><br />
<h2>Query String Parameters</h2>
<$=JSON.stringify(formbody)$><br /><br />
<$=JSON.stringify(formbody.filecontent.replace(/\r/g,''))$>
</body>
</html>
